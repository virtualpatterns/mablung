  "main": "index.js",
  "name": "mablung",
  "repository": {
    "type": "git",
    "url": "https://github.com/virtualpatterns/library"
  },
  "scripts": {
    "link": "hln ./process/logs \"$HOME/Library/Logs/mablung\"",
    "unlink": "hln -u \"$HOME/Library/Logs/mablung\"",
    "upgrade": "npm-check-updates --upgrade",
    "clean": "rm -rf ./coverage ./library ./server ./tests ./www",
    "lint": "eslint ./resources ./source",
    "build": "npm run clean && babel ./source --copy-files --out-dir . && webpack",
    "test": "npm run test-library && npm run test-www",
    "test-library": "npm run build && mocha --bail --compilers js:babel-core/register --timeout 0 ./tests",
    "test-www": "npm run build && mocha-phantomjs --bail --hooks ./resources/hooks.js --ignore-resource-errors --path ./node_modules/.bin/phantomjs http://localhost:8082/www/test.html",
    "coverage-library": "npm run build && istanbul cover ./node_modules/.bin/_mocha --dir ./coverage -- --bail --timeout 0 ./tests",
    "run": "npm run build && node server/index.js"
  },
  "version": "0.0.11",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.22.0",
    "eslint": "^3.16.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mocha-phantomjs": "^4.1.0",
    "npm-check-updates": "^2.10.3",
    "restify": "^4.3.0",
    "sinon": "^1.17.7",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "detect-node": "^2.0.3",
    "es6-promisify": "^5.0.0",
    "is-relative": "^0.2.1",
    "pad": "^1.0.2",
    "touch": "^1.0.0",
    "winston": "^2.3.1"
  }
}
