  },
  "scripts": {
    "clean": "rm -r -f distributables/configuration.js distributables/tasks",
    "lint": "npm run clean && eslint --ignore-path .gitignore source/tasks",
    "build": "npm run lint && babel source/configuration.js --out-file distributables/configuration.js --source-maps inline && babel source/webpack.configuration.js --out-file webpack.configuration.js --source-maps inline && babel source/tasks --copy-files --out-dir distributables/tasks --source-maps inline",
    "tasks": "npm run build && jake --jakefile distributables/tasks/index.js --tasks",
    "jake": "npm run build && jake --jakefile distributables/tasks/index.js",
    "upgrade": "npm-check-updates --upgrade"
  },
  "version": "0.0.12",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.15.0",
    "jake": "^8.0.15",
    "mocha": "^5.0.0",
    "npm-check-updates": "^2.10.3",
    "restify": "^6.3.4",
    "restify-plugins": "^1.6.0",
    "sinon": "^4.1.6",
    "webpack": "^3.10.0"
  },
  "dependencies": {
    "@pwn/is": "^1.0.2",
    "babel-polyfill": "^6.26.0",
    "detect-node": "^2.0.3",
    "es6-promisify": "^5.0.0",
    "is-relative": "^1.0.0",
    "mkdirp": "^0.5.1",
    "pad": "^2.0.3",
    "pino": "^4.10.3",
    "touch": "^3.1.0"
  }
}
